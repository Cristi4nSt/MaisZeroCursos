@model List<DiscentesModel>

<style>
    table {
        min-height: 400px !important;
        height: 100px !important;
    }
</style>

<table class="table table-striped table-responsive align-middle mb-0 bg-white">
    <thead class="bg-light">
        <tr>
            <th>Nome</th>

            <th>Número do CPF</th>

            <th>Sexo do discente</th>

            <th>Ação</th>
        </tr>
    </thead>

    <tbody class=".table-hover">
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.Cpf</td>
                <td>@item.SexoDiscente</td>

                <td>
                    <button type="button" onclick="" class="btn btn-link btn-rounded btn-sm fw-bold">
                        Editar
                    </button>
                </td>
            </tr>
        }
    </tbody>
</table>